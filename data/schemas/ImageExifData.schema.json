{
    "$schema": "http://json-schema.org/draft-07/schema#",
    "type": "object",
    "properties": {
        "pk": {
            "type": "string"
        },
        "sk": {
            "type": "string",
            "pattern": "^image#v[0-9]+$"
        },
        "file_type": {
            "type": ["string", "null"]
        },
        "compression": {
            "type": ["string", "null"]
        },
        "jpeg": {
            "type": ["boolean", "null"]
        },
        "length": {
            "type": ["integer", "null"]
        },
        "width": {
            "type": ["integer", "null"]
        },
        "orientation": {
            "type": ["string", "null"]
        },
        "date_time": {
            "type": ["string", "null"]
        },
        "date_time_offset": {
            "type": ["string", "null"]
        },
        "auto_focus": {
            "type": ["boolean", "null"]
        },
        "exposure_mode": {
            "type": ["string", "null"]
        },
        "exposure_program": {
            "type": ["string", "null"]
        },
        "exposure_time": {
            "type": ["number", "null"]
        },
        "flash": {
            "type": ["string", "null"]
        },
        "fnumber": {
            "type": ["number", "null"]
        },
        "focal_length": {
            "type": ["number", "null"]
        },
        "focal_length_in_35mm_film": {
            "type": ["integer", "null"]
        },
        "photographic_sensitivity": {
            "type": ["array", "null"],
            "items": {
                "type": "integer"
            }
        },
        "light_source": {
            "type": ["string", "null"]
        },
        "metering_mode": {
            "type": ["string", "null"]
        },
        "sensing_method": {
            "type": ["string", "null"]
        },
        "sensitivity_type": {
            "type": ["string", "null"]
        },
        "contrast": {
            "type": ["string", "null"]
        },
        "gain_control": {
            "type": ["string", "null"]
        },
        "saturation": {
            "type": ["string", "null"]
        },
        "sharpness": {
            "type": ["string", "null"]
        },
        "subject_distance_range": {
            "type": ["string", "null"]
        },
        "white_balance": {
            "type": ["string", "null"]
        }
    },
    "required": [
       "file_type",
        "compression",
        "jpeg",
        "length",
        "width",
        "orientation",
        "date_time",
        "date_time_offset",
        "auto_focus",
        "exposure_mode",
        "exposure_program",
        "exposure_time",
        "flash",
        "fnumber",
        "focal_length",
        "focal_length_in_35mm_film",
        "photographic_sensitivity",
        "light_source",
        "metering_mode",
        "sensing_method",
        "sensitivity_type",
        "contrast",
        "gain_control",
        "saturation",
        "sharpness",
        "subject_distance_range",
        "white_balance"
    ],
    "additionalProperties": false,
    "dependencies": {
        "pk": [ "sk" ],
        "sk": [ "pk" ]
    }
}
