{
    "$schema": "http://json-schema.org/draft-07/schema#",
    "$id": "/schemas/cameraData",
    "type": "object",
    "properties": {
        "pk": {
            "type": "string"
        },
        "sk": {
            "type": "string",
            "pattern": "^camera#v[0-9]+$"
        },
        "make": {
            "type": "string"
        },
        "model": {
            "type": "string"
        },
        "software": {
            "type": "string"
        },
        "serial_number": {
            "type": "string"
        }
    },
    "required": [
        "make",
        "model",
        "software",
        "serial_number"
    ],
    "additionalProperties": false,
    "dependencies": {
        "pk": [ "sk" ],
        "sk": [ "pk" ]
    }
}
