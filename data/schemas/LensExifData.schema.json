{
    "$schema": "http://json-schema.org/draft-07/schema#",
    "$id": "/schemas/lensData",
    "type": "object",
    "properties": {
        "pk": {
            "type": "string"
        },
        "sk": {
            "type": "string",
            "pattern": "^lens#v[0-9]+$"
        },
        "make": {
            "type": "string"
        },
        "model": {
            "type": "string"
        },
        "serial_number": {
            "type": "string"
        },
        "min_focal": {
            "type": "integer"
        },
        "max_focal": {
            "type": "integer"
        },
        "min_aperture": {
            "type": "number"
        },
        "max_aperture_high": {
            "type": "number"
        },
        "max_aperture_low": {
            "type": "number"
        },
        "auto_focus": {
            "type": "boolean"
        },
        "vibration_reduction": {
            "type": "boolean"
        },
        "lens_maker_type": {
            "type": "array"
        },
        "camera_maker_type": {
            "type": "array",
            "items": {
                "type": "string"
            }
        },
        "macro": {
            "type": "boolean"
        },
        "zoom": {
            "type": "boolean"
        }
    },
    "required": [
        "make",
        "model",
        "serial_number",
        "min_focal",
        "max_focal",
        "min_aperture",
        "max_aperture_high",
        "max_aperture_low",
        "auto_focus",
        "vibration_reduction",
        "lens_maker_type",
        "camera_maker_type",
        "macro",
        "zoom"
    ],
    "additionalProperties": false,
    "dependencies": {
        "pk": [ "sk" ],
        "sk": [ "pk" ]
    }
}
